<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/backed">

    <EditText
        android:id="@+id/quiz1"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/back"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/quiz3"
        app:layout_constraintEnd_toEndOf="@+id/quiz2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/quiz2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.375" />

    <EditText
        android:id="@+id/quiz2"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/back"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/quiz3"
        app:layout_constraintEnd_toEndOf="@+id/quiz3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/quiz3"
        app:layout_constraintTop_toBottomOf="@+id/quiz1"
        app:layout_constraintVertical_bias="0.375" />

    <EditText
        android:id="@+id/quiz3"
        android:layout_width="100dp"
        android:layout_height="49dp"
        android:layout_marginBottom="12dp"
        android:autofillHints=""
        android:background="@drawable/back"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/quiz4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.321"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/quiz4"
        android:layout_width="105dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:background="@drawable/back"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/mid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.326"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.853" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/calcc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.793" />

    <TextView
        android:id="@+id/answer"
        android:layout_width="0dp"
        android:layout_height="103dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@android:color/transparent"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.978"
        android:fontFamily="sans-serif-medium"/>

    <EditText
        android:id="@+id/mid"
        android:layout_width="101dp"
        android:layout_height="48dp"
        android:background="@drawable/back"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <EditText
        android:id="@+id/Attendance"
        android:layout_width="101dp"
        android:layout_height="48dp"
        android:background="@drawable/back"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.594" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/quiz1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toEndOf="@+id/quiz1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/quiz1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/quiz2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toEndOf="@+id/quiz2"
        app:layout_constraintTop_toBottomOf="@+id/quiz1"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/quiz2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/quiz3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toEndOf="@+id/quiz3"
        app:layout_constraintTop_toBottomOf="@+id/quiz2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/quiz3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/quiz4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toEndOf="@+id/quiz4"
        app:layout_constraintTop_toBottomOf="@+id/quiz3"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/quiz4" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="89dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/mid"
        app:layout_constraintEnd_toStartOf="@+id/mid"
        app:layout_constraintHorizontal_bias="0.74"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/mid"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/mid" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/Attendance"
        app:layout_constraintEnd_toStartOf="@+id/Attendance"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Attendance"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/attendance" />

</androidx.constraintlayout.widget.ConstraintLayout>